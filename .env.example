# YandexMetrica Configuration
# Copy this file to .env and fill in your actual values

# Required: Authentication token for the API service
TAP_YANDEX_METRICA_AUTH_TOKEN=your_auth_token_here

# Required: Project IDs to replicate (comma-separated)
TAP_YANDEX_METRICA_PROJECT_IDS=project1,project2,project3

# Optional: The earliest record date to sync (ISO format)
TAP_YANDEX_METRICA_START_DATE=2023-01-01T00:00:00Z

# Optional: API URL (defaults to https://api.mysample.com)
TAP_YANDEX_METRICA_API_URL=https://api.mysample.com

# Optional: Custom User-Agent header
TAP_YANDEX_METRICA_USER_AGENT=tap-yandex-metrica/1.0.0



TAP_YANDEX_METRICA_AUTH_TOKEN=""
TAP_YANDEX_METRICA_COUNTER_ID=""
TAP_YANDEX_METRICA_START_DATE="2025-12-01"
TAP_YANDEX_METRICA_END_DATE="2025-12-01"
TAP_YANDEX_METRICA_DAYS_AGO="2"
